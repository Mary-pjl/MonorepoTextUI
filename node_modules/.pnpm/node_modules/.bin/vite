#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/e/huichuang/test-ui/node_modules/.pnpm/vite@5.4.21_@types+node@24._441fea0e96634e67cfe82fa525e18ecc/node_modules/vite/bin/node_modules:/mnt/e/huichuang/test-ui/node_modules/.pnpm/vite@5.4.21_@types+node@24._441fea0e96634e67cfe82fa525e18ecc/node_modules/vite/node_modules:/mnt/e/huichuang/test-ui/node_modules/.pnpm/vite@5.4.21_@types+node@24._441fea0e96634e67cfe82fa525e18ecc/node_modules:/mnt/e/huichuang/test-ui/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/e/huichuang/test-ui/node_modules/.pnpm/vite@5.4.21_@types+node@24._441fea0e96634e67cfe82fa525e18ecc/node_modules/vite/bin/node_modules:/mnt/e/huichuang/test-ui/node_modules/.pnpm/vite@5.4.21_@types+node@24._441fea0e96634e67cfe82fa525e18ecc/node_modules/vite/node_modules:/mnt/e/huichuang/test-ui/node_modules/.pnpm/vite@5.4.21_@types+node@24._441fea0e96634e67cfe82fa525e18ecc/node_modules:/mnt/e/huichuang/test-ui/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../vite/bin/vite.js" "$@"
else
  exec node  "$basedir/../vite/bin/vite.js" "$@"
fi
